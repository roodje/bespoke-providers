{
  "request": {
    "method": "POST",
    "urlPathPattern": "/v1/consents",
    "headers": {
      "X-Request-ID": {
        "matches": "^[a-zA-Z0-9]{8}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{12}$"
      },
      "Signature": {
        "matches": "keyId=\"SN=13299021239615735660,CA=C=US,ST=foo,L=foo,O=foo,OU=foo,CN=foo\",algorithm=\"rsa-sha256\",headers=\"digest psu-id x-request-id\",signature=\"(.*)\""
      },
      "Digest": {
        "matches": "SHA-256=(.*)"
      },
      "TPP-Signature-Certificate": {
        "matches": "^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$"
      },
      "PSU-ID": {
        "equalTo": "John"
      },
      "PSU-IP-Address": {
        "equalTo": "127.0.0.1"
      },
      "content-type": {
        "equalTo": "application/json"
      },
      "accept": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.access[?(@.allPsd2 == 'allAccounts')]"
      },
      {
        "matchesJsonPath": "$[?(@.recurringIndicator == true)]"
      },
      {
        "matchesJsonPath": "$[?(@.combinedServiceIndicator == false)]"
      },
      {
        "matchesJsonPath": "$[?(@.validUntil == '2022-03-31')]"
      },
      {
        "matchesJsonPath": "$[?(@.frequencyPerDay == 4)]"
      }
    ]
  },
  "response": {
    "status": 201,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "consentStatus": "received",
      "consentId": "1234-wertiq-983",
      "_links": {
        "startAuthorisationWithPsuAuthentication": {
          "href": "/v1/consents/1234-wertiq-983/authorisations"
        }
      }
    }
  }
}
