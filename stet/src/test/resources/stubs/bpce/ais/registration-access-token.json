{
  "request": {
    "method": "POST",
    "urlPattern": "/stet/psd2/oauth/token",
    "headers": {
      "Content-Type": {
        "contains": "application/x-www-form-urlencoded;charset=UTF-8"
      }
    },
    "bodyPatterns": [
      {
        "equalTo": "grant_type=client_credentials&scope=manageRegistration&client_id=PSD2_TPPRegister"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "X-Frame-Options": "DENY",
      "Transfer-Encoding": "chunked",
      "Content-Type": "application/json;charset=UTF-8"
    },
    "jsonBody": {
      "access_token": "KXyZabcdefghijklmopqrstuvw1234567890",
      "token_type": "Bearer",
      "expires_in": 3600,
      "scope": "manageRegistration"
    }
  }
}
