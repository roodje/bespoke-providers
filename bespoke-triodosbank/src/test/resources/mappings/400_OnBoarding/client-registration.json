{
  "request": {
    "method": "POST",
    "urlPathPattern": "/auth/(uk|nl|be_fr|be_nl)/v1/registration",
    "headers": {
      "accept": {
        "equalTo": "application/json"
      },
      "content-type": {
        "contains": "application/x-www-form-urlencoded"
      },
      "tpp-signature-certificate": {
        "equalTo": ""
      },
      "authorization": {
        "equalTo": "Bearer 11111111-1111-0000-0000-000000000000"
      }
    },
    "bodyPatterns": [
      {
        "contains": "initial_access_token=11111111-1111-0000-0000-000000000000"
      },
      {
        "contains": "sector_identifier_uri="
      },
      {
        "contains": "redirect_uris=https%3A%2F%2Fyolt.com%2Fcallback-acc"
      },
      {
        "contains": "redirect_uris=https%3A%2F%2Fyolt.com%2dev"
      }
    ]
  },
  "response": {
    "status": 400
  }
}
