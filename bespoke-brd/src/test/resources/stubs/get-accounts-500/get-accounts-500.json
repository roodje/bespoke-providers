{
  "request": {
    "method": "GET",
    "urlPathPattern": "/v1/accounts",
    "headers": {
      "x-request-id": {
        "matches": "^[a-zA-Z0-9]{8}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{12}$"
      },
      "psu-ip-address": {
        "equalTo": "127.0.0.1"
      },
      "accept": {
        "equalTo": "application/json"
      },
      "consent-id": {
        "equalTo": "800000022"
      }
    }
  },
  "response": {
    "status": 500,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "code": "500",
      "category": "ERROR",
      "tppMessages": "Internal Server Error"
    }
  }
}
