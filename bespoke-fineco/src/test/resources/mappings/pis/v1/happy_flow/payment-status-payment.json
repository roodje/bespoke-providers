{
  "request": {
    "method": "GET",
    "urlPathPattern": "/v1/payments/sepa-credit-transfers/StatusPaymentId/status",
    "headers": {
      "Content-Type": {
        "matches": "application/json"
      },
      "X-Request-ID": {
        "matches": "^[a-zA-Z0-9]{8}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{4}-[a-zA-Z0-9]{12}$"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "transactionStatus": "ACCP",
      "fundsAvailable": true
    }
  }
}
